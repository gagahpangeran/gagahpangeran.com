{
  "name": "gpr",
  "private": true,
  "description": "GPR's website built using Gatsby and Markdown",
  "version": "0.1.0",
  "author": "GPR <gpr@gagahpangeran.com>",
  "scripts": {
    "build": "gatsby build",
    "build:clean": "rm -rf ./public && rm -rf ./.cache && yarn build",
    "develop": "gatsby develop",
    "format": "yarn format:prettier && yarn format:eslint",
    "format:eslint": "eslint --fix  \"**/*.{ts,tsx}\"",
    "format:prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,scss}\"",
    "install:clean": "rm -rf ./node_modules && yarn",
    "lint": "yarn lint:prettier && yarn lint:eslint",
    "lint:eslint": "eslint --quiet \"**/*.{ts,tsx}\"",
    "lint:prettier": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,scss}\"",
    "start": "gatsby develop",
    "serve": "gatsby serve",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:cov": "yarn test --coverage",
    "prepare": "husky install"
  },
  "dependencies": {
    "@fontsource/fira-code": "^4.5.2",
    "@fontsource/lato": "^4.5.0",
    "@fontsource/rubik": "^4.5.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "gatsby": "^3.14.0",
    "gatsby-plugin-catch-links": "^3.14.0",
    "gatsby-plugin-image": "^1.14.0",
    "gatsby-plugin-manifest": "^3.14.0",
    "gatsby-plugin-offline": "^4.14.0",
    "gatsby-plugin-react-helmet": "^4.14.0",
    "gatsby-plugin-sass": "^4.14.0",
    "gatsby-plugin-sharp": "^3.14.0",
    "gatsby-plugin-typegen": "^2.2.4",
    "gatsby-plugin-typescript": "^3.14.0",
    "gatsby-remark-copy-linked-files": "^4.11.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^5.11.0",
    "gatsby-remark-prismjs": "^5.11.0",
    "gatsby-remark-responsive-iframe": "^4.11.0",
    "gatsby-remark-smartypants": "^4.11.0",
    "gatsby-source-filesystem": "^3.14.0",
    "gatsby-transformer-remark": "^4.11.0",
    "gatsby-transformer-sharp": "^3.14.0",
    "lodash.kebabcase": "^4.1.1",
    "prismjs": "^1.25.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "sass": "^1.42.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.3",
    "@types/lodash.kebabcase": "^4.1.6",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^17.0.11",
    "@types/react-helmet": "^6.1.4",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "babel-jest": "^27.4.5",
    "babel-preset-gatsby": "^2.4.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.27.1",
    "husky": "^7.0.4",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.4.5",
    "lint-staged": "^12.1.2",
    "prettier": "^2.5.1",
    "typescript": "^4.5.4"
  },
  "resolutions": {
    "graphql": "^15.0.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,scss}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gagahpangeran/gagahpangeran.com"
  },
  "bugs": {
    "url": "https://github.com/gagahpangeran/gagahpangeran.com/issues"
  }
}
