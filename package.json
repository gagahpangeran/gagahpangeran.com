{
  "name": "gpr",
  "private": true,
  "description": "GPR's website built using Gatsby and Markdown",
  "version": "0.1.0",
  "author": "GPR <gpr@gagahpangeran.com>",
  "scripts": {
    "build": "gatsby build",
    "build:clean": "yarn clean && yarn build",
    "clean": "gatsby clean",
    "develop": "gatsby develop",
    "format": "yarn format:prettier && yarn format:eslint",
    "format:eslint": "eslint --fix  \"**/*.{ts,tsx}\"",
    "format:prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md,scss}\"",
    "install:clean": "rm -rf ./node_modules && yarn",
    "lint": "yarn lint:prettier && yarn lint:eslint",
    "lint:eslint": "eslint --quiet \"**/*.{ts,tsx}\"",
    "lint:prettier": "prettier --check \"**/*.{js,jsx,ts,tsx,json,md,scss}\"",
    "prepare": "husky install",
    "start": "gatsby develop",
    "start:clean": "yarn clean && yarn start",
    "serve": "gatsby serve",
    "type-check": "tsc --noEmit",
    "test": "jest",
    "test:cov": "yarn test --coverage"
  },
  "dependencies": {
    "@fontsource/fira-code": "^4.5.11",
    "@fontsource/lato": "^4.5.9",
    "@fontsource/rubik": "^4.5.11",
    "@fortawesome/fontawesome-svg-core": "^6.2.0",
    "@fortawesome/free-solid-svg-icons": "^6.2.0",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "gatsby": "^4.23.1",
    "gatsby-plugin-catch-links": "^4.23.0",
    "gatsby-plugin-image": "^2.23.1",
    "gatsby-plugin-manifest": "^4.23.1",
    "gatsby-plugin-netlify": "^5.0.1",
    "gatsby-plugin-offline": "^5.23.1",
    "gatsby-plugin-sass": "^5.23.1",
    "gatsby-plugin-sharp": "^4.23.1",
    "gatsby-remark-autolink-headers": "^5.24.0",
    "gatsby-remark-copy-linked-files": "^5.23.0",
    "gatsby-remark-external-links": "^0.0.4",
    "gatsby-remark-images": "^6.23.1",
    "gatsby-remark-prismjs": "^6.23.0",
    "gatsby-remark-responsive-iframe": "^5.23.0",
    "gatsby-remark-smartypants": "^5.23.0",
    "gatsby-source-filesystem": "^4.23.0",
    "gatsby-transformer-remark": "^5.23.1",
    "gatsby-transformer-sharp": "^4.23.1",
    "lodash.kebabcase": "^4.1.1",
    "prismjs": "^1.29.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-markdown": "^8.0.3",
    "remark-github": "^11.2.4",
    "sass": "^1.55.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.18.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@types/jest": "^29.0.3",
    "@types/lodash.kebabcase": "^4.1.7",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.38.0",
    "@typescript-eslint/parser": "^5.38.0",
    "babel-jest": "^29.0.3",
    "babel-preset-gatsby": "^2.23.0",
    "eslint": "^8.23.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "husky": "^8.0.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.0.3",
    "jest-environment-jsdom": "^29.0.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "typescript": "^4.8.3"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,md,scss}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gagahpangeran/gagahpangeran.com"
  },
  "bugs": {
    "url": "https://github.com/gagahpangeran/gagahpangeran.com/issues"
  }
}
